<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.apache.ibatis.submitted.imports.Mapper"
    extends="org.apache.ibatis.submitted.imports.extends">

    <import src="org.apache.ibatis.submitted.imports.imports"/>


    <resultMap id="userMapFromImports" extends="baseMap-imports"
              type ="org.apache.ibatis.submitted.imports.User">
        <result property="age"  column="age"/>
    </resultMap>

    <select id="getUserListByImport" resultMap="userMapFromImports">
        select id, name ,age from users
    </select>


    <resultMap id="companyMapFromExtends" extends="baseMap-extends"
               type ="org.apache.ibatis.submitted.imports.Company">
        <result property="asset"  column="asset"/>
    </resultMap>

    <select id="getCompanyListByExtends" resultMap="companyMapFromExtends">
        select id, name , asset from company
    </select>


</mapper>
