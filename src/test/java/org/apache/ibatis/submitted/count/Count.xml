<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="org.apache.ibatis.submitted.count.CountMapper">
  <select id="count" resultType="int">
    select count(*)
    from names
  </select>
</mapper>
