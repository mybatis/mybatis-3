<!DOCTYPE sqlMap
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap>

  <select id="mapBeanMap"
          parameterClass="map"
          resultClass="int">
    select ACC_ID from Account where ACC_ID = #bean.map.id#
  </select>

</sqlMap>